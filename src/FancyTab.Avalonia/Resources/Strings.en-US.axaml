    <sys:String x:Key="HelpTip_Title">Quick Start:</sys:String>
    <sys:String x:Key="HelpTip_Content">Press 0-9 for fret | Arrow keys to move | H hammer P pull B bend | F1 for shortcuts</sys:String>
    <sys:String x:Key="HelpTip_Close">Close tip</sys:String>

