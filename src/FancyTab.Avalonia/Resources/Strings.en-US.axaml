    <sys:String x:Key="Shortcuts_W">W - Whole note</sys:String>
    <sys:String x:Key="Shortcuts_Q">Q - Quarter note</sys:String>
    <sys:String x:Key="Shortcuts_E">E - Eighth note</sys:String>

